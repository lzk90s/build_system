##	system target
.PHONY: main clean distclean

main: $(LIST.TARGET_ALL)

clean: $(LIST.TARGET_CLEAN)

distclean:
	$(RM) $(PROJECT_OUTPUT_TOP_DIR)
	
install:
	@echo "no support for this now !"